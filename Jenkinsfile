pipeline{
         agent {
                 label "built-in"
                }
         stages {
            stage ("stage1"){
                   steps {
                          sh "yum install httpd -y"
                          
}
}

           agent {
                    label "slave-1"
                  }

           
                     stage ("onslave"){
                       steps{
                        echo "helo"
}

}




}

